{
    "name": "mychannel",
    "version": "1.0",
    "client": {
      "organization": "Org2",
      "connection": {
        "timeout": {
          "peer": {
            "endorser": "300",
            "eventHub": "300",
            "eventReg": "300"
          },
          "orderer": "300"
        }
      }
    },
    "channels": {
      "mychannel": {
        "orderers": [
          "orderer.example.com"
        ],
        "peers": {
          "peer0.org2.example.com": {
            "endorsingPeer": true,
            "chaincodeQuery": true,
            "ledgerQuery": true,
            "eventSource": true,
            "discover": true
          }
        }
      }
    },
    "organizations": {
      "Org2": {
        "mspid": "Org2MSP",
        "peers": [
          "peer0.org2.example.com"
        ],
        "certificateAuthorities": [
          "ca.org2.example.com"
        ]
      }
    },
    "orderers": {
      "orderer.example.com": {
        "url": "grpcs://localhost:7050",
        "grpcOptions": {
          "ssl-target-name-override": "orderer.example.com"
        },
        "tlsCACerts": {
          "path": "../network/test-network/organizations/ordererOrganizations/example.com/tlsca/tlsca.example.com-cert.pem"
        }
      }
    },
    "peers": {
      "peer0.org2.example.com": {
        "url": "grpcs://localhost:9051",
        "grpcOptions": {
          "ssl-target-name-override": "peer0.org2.example.com"
        },
        "tlsCACerts": {
          "path": "../network/test-network/organizations/peerOrganizations/org2.example.com/tlsca/tlsca.org2.example.com-cert.pem"
        }
      }
    },
    "certificateAuthorities": {
      "ca.org2.example.com": {
        "url": "https://localhost:8054",
        "caName": "ca.org2.example.com",
        "httpOptions": {
          "verify": false
        },
        "tlsCACerts": {
          "path": "../network/test-network/organizations/peerOrganizations/org2.example.com/ca/ca.org2.example.com-cert.pem"
        }
      }
    }
  }